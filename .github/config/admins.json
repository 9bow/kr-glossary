      "description": "새로운 용어 추가는 해당 분야 전문가 1명의 승인 필요"
    },
    "term-modification": {
      "min_approvals": 1,
      "required_roles": ["reviewer", "maintainer", "owner"],
      "specialization_match": false,
      "description": "기존 용어 수정은 모든 분야 관리자의 승인 가능"
    },
    "contributor-addition": {
      "min_approvals": 1,
      "required_roles": ["maintainer", "owner"],
      "specialization_match": false,
      "description": "기여자 추가는 maintainer 이상의 승인 필요"
    },
    "admin-addition": {
      "min_approvals": 2,
      "required_roles": ["owner"],
      "specialization_match": false,
      "description": "관리자 추가는 owner 2명의 승인 필요"
    },
    "organization-addition": {
      "min_approvals": 1,
      "required_roles": ["maintainer", "owner"],
